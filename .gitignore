# dependencies
/node_modules
/.pnp
.pnp.js

# testing
/coverage

# production
/build

# misc
.DS_Store
.env.local
.env.development.local
.env.test.local
.env.production.local

npm-debug.log*
yarn-debug.log*
yarn-error.log*


# Ignore Terraform state files and backups
*.tfstate
*.tfstate.backup

# Ignore Terraform crash logs
crash.log

# Ignore Terraform lock file (ensures others can init with latest dependencies)
.terraform.lock.hcl

# Ignore Terraform directories and cache
.terraform/
terraform.tfvars
terraform.tfvars.json

# Ignore override files (used for local customization)
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Ignore sensitive environment variable files
.envrc
.env

# Ignore generated files (example for workspaces)
terraform.tfstate.d/

# Ignore plan files (generated from `terraform plan -out=planfile`)
planfile
planfile.*
